{
    "name": "prover",
    "version": "0.0.1",
    "publisher": "knrafto",
    "engines": {
        "vscode": "^1.40.0"
    },
    "displayName": "prover",
    "description": "",
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:prover"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "prover",
                "aliases": [
                    "Prover",
                    "prover"
                ],
                "extensions": [
                    ".pf"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "prover",
                "scopeName": "source.prover",
                "path": "./syntaxes/prover.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "tslint": "^5.20.0",
        "vscode-test": "^1.2.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/knrafto/prover.git"
    }
}
